	"version": "0.2.0",
	"configurations": [
		{
            "type": "chrome",
            "request": "attach",
            "name": "paper",
            "port": 9222,
            "webRoot": "${workspaceFolder}",
            "url": "http://localhost:8080/",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "node_modules/**"
            ],
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack-internal:///*": "${webRoot}/*"
            }
        }
	]
}